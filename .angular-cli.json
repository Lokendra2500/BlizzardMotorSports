{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "blizzard-moter-sports"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "./assets/files/css/style.css",
        "./assets/files/css/font-awesome.min.css",
        "./assets/files/css/ionicons.css",
        "./assets/files/revolution/css/settings.css",
        "./assets/files/revolution/css/layers.css",
        "./assets/files/revolution/css/navigation.css",
        "./assets/files/css/owl.carousel.css",
        "./assets/files/css/lightcase.css",
        "./assets/files/css/isotope.css",
        "./assets/files/css/mqueries.css"
      ],
      "scripts": [
        "./assets/files/js/jquery-2.1.4.min.js",
        "./assets/files/js/jquery.easing.1.3.js",
        "./assets/files/js/jquery.visible.min.js",
        "./assets/files/revolution/js/jquery.themepunch.tools.min838f.js",
        "./assets/files/revolution/js/jquery.themepunch.revolution.min838f.js",
        "./assets/files/revolution/js/extensions/revolution.extension.slideanims.min.js",
        "./assets/files/revolution/js/extensions/revolution.extension.layeranimation.min.js",
        "./assets/files/revolution/js/extensions/revolution.extension.navigation.min.js",
        "./assets/files/revolution/js/extensions/revolution.extension.video.min.js",
        "./assets/files/revolution/js/extensions/revolution.extension.actions.min.js",
        "./assets/files/js/tweenMax.js",
        "./assets/files/js/jquery.backgroundparallax.min.js",
        "./assets/files/js/jquery.isotope.min.js",
        "./assets/files/js/jquery.imagesloaded.min.js",
        "./assets/files/js/jquery.owl.carousel.js",
        "./assets/files/js/jquery.lightcase.min.js",
        "./assets/files/js/script.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
